<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="PopupDAO">
	<select id="getPopupList" resultType="popup">
	 	select *
	 	from popup
	 	order by pop_no desc
	</select>
  </mapper>